{
  "name": "@most/behave",
  "description": "behave",
  "version": "1.0.0",
  "author": "Brian Cavalier <brian@briancavalier.com> (github.com/briancavalier)",
  "bugs": {
    "url": "https://github.com/mostjs/behave/issues"
  },
  "devDependencies": {
    "@most/dom-event": "^1.3.2",
    "@most/eslint-config-most": "^1.0.3",
    "assert": "^1.4.1",
    "babel-eslint": "^7.1.1",
    "buba": "^4.0.1",
    "cpy-cli": "^1.0.1",
    "eslint": "^3.12.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "flow-bin": "^0.37.4",
    "istanbul": "^1.1.0-alpha.1",
    "jsinspect": "^0.8.0",
    "mocha": "^3.2.0",
    "most": "^1.1.1",
    "rollup": "^0.37.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^6.0.0",
    "rollup-plugin-node-resolve": "^2.0.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/mostjs/behave#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mostjs/behave.git"
  },
  "scripts": {
    "build:add-inputs": "rollup -c examples/rollup.config.js -o examples/add-inputs/app.js examples/add-inputs/index.js",
    "build:simple-clock": "rollup -c examples/rollup.config.js -o examples/simple-clock/app.js examples/simple-clock/index.js",
    "build": "npm run build:dist",
    "build:dist": "mkdir -p dist && rollup -c && cpy src/index.js.flow dist",
    "lint": "jsinspect src && jsinspect test && eslint src test",
    "prepublish": "npm run build",
    "test": "npm run lint && npm run unit-test",
    "unit-test": "istanbul cover _mocha"
  },
  "dependencies": {
    "@most/core": "^0.2.0",
    "@most/multicast": "^1.2.5",
    "@most/prelude": "^1.4.1"
  }
}
