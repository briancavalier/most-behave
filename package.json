{
  "name": "@most/behave",
  "description": "behave",
  "version": "1.0.0",
  "author": "Brian Cavalier <brian@briancavalier.com> (github.com/briancavalier)",
  "bugs": {
    "url": "https://github.com/briancavalier/most-behave/issues"
  },
  "devDependencies": {
    "@briancavalier/assert": "^3.4.0",
    "@most/dom-event": "^1.3.2",
    "babel-eslint": "^7.2.3",
    "buba": "^4.0.2",
    "cpy-cli": "^1.0.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.46.0",
    "jsinspect": "^0.8.0",
    "mocha": "^3.3.0",
    "most": "^1.3.0",
    "nyc": "^10.3.2",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/mostjs/behave#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/briancavalier/most-behave.git"
  },
  "scripts": {
    "build": "npm run build:dist",
    "build:dist": "mkdir -p dist && rollup -c && cpy src/index.js.flow dist && cpy src/index.js.flow dist --rename=index.es.js.flow",
    "build:add-inputs": "rollup -c examples/rollup.config.js -o examples/add-inputs/app.js examples/add-inputs/index.js",
    "build:simple-clock": "rollup -c examples/rollup.config.js -o examples/simple-clock/app.js examples/simple-clock/index.js",
    "test": "npm run test:lint && npm run test:unit",
    "test:unit": "nyc mocha",
    "test:lint": "jsinspect src && jsinspect test && eslint src test",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@most/core": "^0.6.0",
    "@most/prelude": "^1.6.0",
    "@most/scheduler": "^0.5.0",
    "@most/types": "^0.5.0"
  }
}
